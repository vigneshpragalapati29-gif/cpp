#include <iostream>
using namespace std;
void swap(int *a, int *b) {
Date: Roll No.:
Programming with C++ Lab Page No.: 
int temp = *a;
*a = *b;
*b = temp;
}
int main() {
int x = 1;
int y = 2;
cout << "Before swap: x = " << x << " , y = " << y << endl;
swap(&x, &y);
cout << "After swap: x = " << x << ", y = " << y << endl;
return 0;
}
